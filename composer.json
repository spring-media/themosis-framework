{
  "name": "themosis/framework",
  "description": "The Themosis framework.",
  "keywords": ["themosis", "framework", "wordpress"],
  "license": "GPL-2.0-or-later",
  "support": {
    "issues": "https://github.com/themosis/framework/issues",
    "source": "https://github.com/themosis/framework"
  },
  "authors": [
    {
      "name": "Julien Lamb√©",
      "email": "julien@themosis.com",
      "homepage": "https://www.themosis.com/"
    }
  ],
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/skcheung/laravel-framework.git"
    }
  ],
  "autoload": {
    "psr-4": {
      "Themosis\\": "src/"
    },
    "files": [
      "src/Core/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Themosis\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^8.2",
    "composer/installers": "^1.12",
    "dragonmantank/cron-expression": "^3.3.2",
    "filp/whoops": "^2.1",
    "guzzlehttp/guzzle": "^7.2",
    "laravel/framework": "10.x-dev",
    "laravel/sanctum": "^3.2",
    "laravel/tinker": "^2.8",
    "laravel/ui": "^4.2",
    "league/flysystem": "^3.8.0",
    "league/fractal": "^0.20",
    "paragonie/sodium_compat": "^1.15",
    "predis/predis": "^2.1",
    "ramsey/uuid": "^4.7",
    "spatie/enum": "^3.13",
    "spatie/laravel-ignition": "^2.0",
    "symfony/property-access": "^6.2",
    "twig/twig": " ^3.1",
    "vlucas/phpdotenv": "^5.4.1"
  },
  "require-dev": {
    "beyondcode/laravel-dump-server": "^1.7",
    "fakerphp/faker": "^1.9.1",
    "friendsofphp/php-cs-fixer": "^3.8",
    "laravel/pint": "^1.0",
    "laravel/sail": "^1.0.1",
    "mockery/mockery": "^1.4.4",
    "nunomaduro/collision": "^7.0",
    "johnpbloch/wordpress-core": "^6.0",
    "pestphp/pest": "^1.22",
    "pestphp/pest-plugin-laravel": "^1.4",
    "phpunit/phpunit": "^10",
    "symfony/var-dumper": "^6.0"
  },
  "scripts": {
    "test": "phpunit",
    "fix": "php-cs-fixer fix"
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "composer/installers": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
